<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_login">

    <fragment
        android:id="@+id/navigation_login"
        android:name="com.example.embedingsysterm2.ui.login.LoginFragment"
        android:label="@string/title_login"
        tools:layout="@layout/login_view" />

    <fragment
        android:id="@+id/navigation_QRcode"
        android:name="com.example.embedingsysterm2.ui.qrcode.QrcodeFragment"
        android:label="@string/title_QRcode"
        tools:layout="@layout/qr_view" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.embedingsysterm2.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_dbContrl"
        android:name="com.example.embedingsysterm2.database.db"
        android:label="@string/title_dbContrl"
        tools:layout="@layout/fragment_notifications" />

</navigation>